{
    "compilerOptions": {
        "target": "es5",
        "lib": [ "es2015" ],
        "module": "commonjs",
        "moduleResolution": "node",
        "declaration": false,
        "noImplicitAny": true,
        "suppressImplicitAnyIndexErrors": true,
        "removeComments": false,
        "noLib": false,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "sourceMap": true,
        "types": [
            "node", "core-js"
        ]      
    },
    "exclude": [
        "node_modules",
        "client", 
        "client_"
    ]
}